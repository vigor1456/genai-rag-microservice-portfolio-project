{
  "id": null,
  "title": "GenAI Microservice - RAG Ops",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "QPS (5m)",
      "targets": [
        {
          "expr": "sum(rate(requests_total[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Error Rate (5m)",
      "targets": [
        {
          "expr": "sum(rate(errors_total[5m])) / clamp_min(sum(rate(requests_total[5m])), 1e-9)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Latency p95 (s, 5m)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(request_latency_seconds_bucket[5m])) by (le))",
